package day5Projects;
import java.util.*;
public class Display2D {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[][] numbers={ {1,2,3,4},{5,6,7,8}};
		System.out.println("Array of Index 0,0 is "+numbers[0][0]);
		System.out.println("Array of Index 0,1 is "+numbers[0][1]);
		System.out.println("Array of Index 0,2 is "+numbers[0][2]);
		System.out.println("Array of Index 0,3 is "+numbers[0][3]);
		System.out.println("Array of Index 0,0 is "+numbers[0][0]);
		System.out.println("Array of Index 0,0 is "+numbers[1][0]);
		System.out.println("Array of Index 0,0 is "+numbers[2][0]);
		System.out.println("Array of Index 0,0 is "+numbers[3][0]);
		//System.out.println("Array of Index 0,0 is "+numbers[0][0]);
		

	}

}
